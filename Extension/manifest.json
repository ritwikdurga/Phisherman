{
    "manifest_version": 3,
    "name": "Phisherman",
    "version": "1.2",
    "description": "A browser extension to analyzes, detects and blocks phishing websites.",
    "action": {
        "default_popup": "./html/popup.html"
    },
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "./js/background.js"
    }
}